{
  "suggest.floatEnable": false,
  "suggest.autoTrigger": "trigger",
  "suggest.noselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ]
    },
    "vimls": {
      "command": "vim-language-server",
      "args": [
        "--stdio"
      ],
      "initializationOptions": {
        "iskeyword": "vim iskeyword option",
        "vimruntime": "path/to/$VIMRUNTIME",
        "runtimepath": "vim/runtime/path",
        "diagnostic": {
          "enable": true
        }
      },
      "filetypes": [
        "vim"
      ]
    }
  },
  "vimlsp.debug": {
    "type": "boolean",
    "default": false,
    "description": "enable coc-vimlsp service debug"
  },
  "vimlsp.diagnostic.enable": {
    "type": "boolean",
    "default": true,
    "description": "enable diagnostic"
  },
  "vimlsp.trace.server": {
    "type": "string",
    "default": "off",
    "enum": [
      "off",
      "messages",
      "verbose"
    ],
    "description": "Trace level of vim language server"
  },
  "yaml.schemas": {
    "url": "globPattern",
    "kubernetes": "/*.yaml",
    "kedge": "globPattern"
  }
}
